<p id="notice"><%= notice %></p>

<h1>Budgets</h1>

<table>
  <thead>
    <tr>
      <th>Totalvalue</th>
      <th>Education</th>
      <th>Security</th>
      <th>Healt</th>
      <th>Transport</th>
      <th>Infraestructure</th>
      <th>Other</th>
      <th>Cities</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @budgets.each do |budget| %>
      <tr>
        <td><%= budget.totalValue %></td>
        <td><%= budget.education %></td>
        <td><%= budget.security %></td>
        <td><%= budget.healt %></td>
        <td><%= budget.transport %></td>
        <td><%= budget.infraestructure %></td>
        <td><%= budget.other %></td>
        <td><%= budget.cities %></td>
        <td><%= link_to 'Show', budget %></td>
        <td><%= link_to 'Edit', edit_budget_path(budget) %></td>
        <td><%= link_to 'Destroy', budget, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Budget', new_budget_path %>
